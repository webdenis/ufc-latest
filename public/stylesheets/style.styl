body
	padding 20px 45px
	font 14px "Lucida Grande", Helvetica, Arial, sans-serif
	background #000
	color #FFF
h2
	margin-top 40px
a
	color #FFF
	text-decoration none
a:hover
	color #CCC
	text-decoration underline
th
	padding 8px
	text-align left
td
	padding 2px 8px
	transition color 0.5s ease
td:first-child
	color #9b99c1
	padding-right 100px
.wiki
	margin-left 5px
table
	font-size 9pt

table.hideResults
	for i in 4..7
		td:nth-child({i})
			color: #000
.heading
	position absolute
	top 15px
	right 45px
.extend
	font-size 12pt
.showAll
	margin-left 10px
	color #999
	cursor pointer
.getRecord
	cursor pointer
sup a
	font-weight bold